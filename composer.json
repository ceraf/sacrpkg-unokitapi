{
    "type": "symfony-bundle",
    "name": "sacrpkg/unokitapi-bundle",
    "description": "Unokit API Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Bruyako",
            "email": "jonsm2184@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/dotenv": "7.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.*",
        "symfony/yaml": "7.*"
    },
    "require-dev": {
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "sacrpkg\\UnokitapiBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sacrpkg\\UnokitapiBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.*"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
